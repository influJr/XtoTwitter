{
    "manifest_version": 3,
    "name": "XtoTwitter",
    "version": "0.0.1",
    "description": "Replace the X with the Twitter",
    "content_scripts": [
        {
            "matches": ["https://twitter.com/*","https://x.com/*"],
            "js": ["replace.js"]
        }
    ],
    "icons": {
        "128": "icon.png"
    }
}